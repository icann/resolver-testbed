[Unit]
Description={{ service_name }} service
After=syslog.target network-online.target

[Service]
Type=notify
WorkingDirectory={{ software_target_dir }}/etc/knot-resolver
ExecStart={{ software_target_dir }}/sbin/kresd -c {{ software_target_dir }}/etc/knot-resolver/kresd.conf -n

[Install]
WantedBy=multi-user.target
